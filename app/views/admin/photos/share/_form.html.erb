<h2>Upload object</h2>
<%= form_tag(admin_photos_path, method: "post", :multipart=>true ) do %>
  <table class="table table-striped">
    <tr>
      <td><label>Bucket:</label></td>
      <td><%= @bucket.name  %></td>
    </tr>
    <tr>
      <td><label>Prefix:</label></td>
      <td><%= @prefix  %></td>
    </tr>
    <tr>
      <td><label>Select file:</label></td>
      <td><input type="file" name="file" style="display:inline" /></td>
    </tr>
    <tr>
      <td colspan="2">
        
        <input type="submit" class="btn btn-default" direct_upload=true multiple="multiple"/>
        <span>&nbsp;&nbsp</span>
      </td>
    </tr>
  </table>
<% end %>